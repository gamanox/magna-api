{
  "connection": "default",
  "collectionName": "operators",
  "info": {
    "name": "operator"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "operator_number": {
      "type": "string"
    },
    "code_image": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "reports": {
      "via": "employee",
      "collection": "report"
    },
    "assigned_welds": {
      "type": "component",
      "repeatable": true,
      "component": "welds.assigned-welds"
    }
  }
}
